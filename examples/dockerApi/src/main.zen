std: std()
{
    Docker,
    State
    listContainers,
    stopContainer
}: importLocal("./docker.zen")



main: Function()
{
    args: {},
    return: {},
    body: {
        docker: Docker()
        containers: docker.listContainers()
        cLoop: loop(containers) {
            if (cLoop.value.state == State.RUNNING) {
                docker.stopContainer(cLoop.value.id)
            }
        }
    }
}
